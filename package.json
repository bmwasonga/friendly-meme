{
  "name": "take-home",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "yarn start --prefix client",
    "build": "yarn run server && yarn run client",
    "migrate": "yarn run build && npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo:all"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "connect-session-sequelize": "^7.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.17.0"
  },
  "devDependencies": {
    "faker": "^5.5.3",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1"
  }
}